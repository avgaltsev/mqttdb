{
	"root": true,
	"env": {
		"node": true,
		"es2022": true
	},
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"sourceType": "module",
		"project": "./tsconfig.json"
	},
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"@somethings/eslint-config-base",
		"@somethings/eslint-config-typescript"
	],
	"rules": {
	},
	"overrides": [
		{
			"files": ["*.ts"],
			"rules": {
				"@typescript-eslint/no-empty-interface": ["off"]
			}
		}
	]
}
